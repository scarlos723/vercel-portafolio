{
	"env": {
		"browser": true,
		"es2021": true,
		"node": true
	},
	"extends": [
	"plugin:react/recommended",
	"standard"
	],
	"parserOptions": {
		"ecmaFeatures": {
				"jsx": true
		},
		"ecmaVersion": 11,
		"sourceType": "module"
	},
	"plugins": [
		"react"
	],
	"rules": {
		"indent": ["error", 2],
		"react/self-closing-comp":"error", 
		"react/prop-types":"off",
		"react/react-in-jsx-scope":"off",
		"react/jsx-key": "off"
	}
}
